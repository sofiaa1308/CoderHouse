CREATE TABLE LEADS (
IDLEAD INT NOT NULL,
PRIMARY KEY(idlead),
FOREIGN KEY (IDCAR) references STOCK(ID AUTO)
);

CREATE TABLE STOCK (
IDCAR INT NOT NULL,
COLOR VARCHAR(50) NOT NULL,
TRANSMISION VARCHAR(50) NOT NULL,
PRIMARY KEY(IDCAR)
);

CREATE TABLE VISITAS (
IDVISITA INT,
SITE VARCHAR(50),
PRIMARY KEY(IDVISITA),
FOREIGN KEY (IDASESOR) references Equipo_comercial(IDASESOR)
);

CREATE TABLE EQUIPO_COMERCIAL (
IDASESOR INT,
SALES INT,
FECHA_INGRESO DATE
PRIMARY KEY(IDASESOR)
);

CREATE TABLE VENTAS (
PRIMARY KEY (IDVENTA),
PRICE_SALE FLOAT,
FOREIGN KEY (IDAUTO) references STOCK (IDCAR)
);
