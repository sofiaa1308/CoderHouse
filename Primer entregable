CREATE TABLE LEADS (
IDLEAD INT NOT NULL,
IDCAR INT NOT NULL,
PRIMARY KEY(idlead)
);

CREATE TABLE STOCK (
IDCAR INT NOT NULL,
COLOR VARCHAR(50) NOT NULL,
TRANSMISION VARCHAR(50) NOT NULL,
PRIMARY KEY(IDCAR)
);

CREATE TABLE VITAS (
IDVISITA INT,
SITE VARCHAR(50),
IDASESOR VARCHAR(50)
PRIMARY KEY(IDVISITA)
);

CREATE TABLE EQUIPO_COMERCIAL (
IDASESOR INT,
SALES INT,
FECHA_INGRESO DATE
PRIMARY KEY(IDASESOR)
);

CREATE TABLE VENTAS (
IDCAR INT,
PRICE_SALE FLOAT
PRIMARY KEY(IDCAR)
);
