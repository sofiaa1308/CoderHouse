##Tabla5
CREATE TABLE VENTAS (
idventa varchar(55),
PRICE_SALE FLOAT,
idcar varchar(100),
idasesor int,
PRIMARY KEY (idventa),
FOREIGN KEY (idcar) references STOCK (idcar),
FOREIGN KEY (idasesor) references equipo_comercial(idasesor)
);

drop table ventas;

