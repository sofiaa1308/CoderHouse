CREATE TABLE STOCK (
IDCAR VARCHAR(100),
COLOR VARCHAR(50) NOT NULL,
TRANSMISION VARCHAR(50) NOT NULL,
PRIMARY KEY(idcar)
);

CREATE TABLE LEADS (
idlead varchar(100),
idcar varchar(100),
PRIMARY KEY(idlead),
FOREIGN KEY (idcar) references STOCK(idcar)
);

CREATE TABLE EQUIPO_COMERCIAL (
idasesor INT,
SALES INT,
FECHA_INGRESO DATE,
PRIMARY KEY(IDASESOR)
);

CREATE TABLE VISITAS (
idvisita INT,
site VARCHAR(50),
idasesor INT,
PRIMARY KEY(idvisita),
FOREIGN KEY (idasesor) references Equipo_comercial(idasesor)
);


CREATE TABLE VENTAS (
idventa int,
PRICE_SALE FLOAT,
idcar VARCHAR(100),
PRIMARY KEY (idventa),
FOREIGN KEY (idcar) references STOCK (idcar)
);
